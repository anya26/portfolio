        <script type="text/javascript" async="" src="http://www.google-analytics.com/ga.js"></script><script type="text/javascript">
            var currentEndAngle = -.5;
            var currentstartAngle = -.5;
            var currentColor = "white";
            var interval = null;

            function draw(counterClockwise)
            {
                var canvas = document.getElementById("canvas-preloader");
                var context = canvas.getContext("2d");
                var x = canvas.width / 2, y = canvas.height / 2;
                var radius = 32;

                var startAngle = currentstartAngle * Math.PI;
                var endAngle = currentEndAngle * Math.PI;

                currentEndAngle += .01;
                context.clearRect(0, 0, canvas.width, canvas.height);

                context.beginPath();
                context.arc(x,y,radius,startAngle,endAngle,counterClockwise);
                context.lineWidth = 4;
                context.strokeStyle = currentColor;
                context.stroke();

                if(endAngle > 4.5)
                {
                    clearInterval(interval);
                    currentEndAngle = -.5;
                    interval = setInterval(draw,1,!counterClockwise);
                }
            }

            function stopDraw()
            {
                clearInterval(interval);
            }

            interval = setInterval(draw,1,false);

        </script>


